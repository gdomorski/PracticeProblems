//Here is an example of how to count letters in a string with the reduce callback


function letterCount (str, letter){
    
var counter = 0;

    reduce(str, function(a, b){
       
       if(a === letter){
            counter++;
       }
       
       if(b === letter){
           counter++;
       }
        
    });
    
    return counter;
    
}


letterCount('aaaabbbbcccccdde', 'b');
